Package: BGFD
Type: Package
Title: Bell-G and Complementary Bell-G Family of Distributions
Version: 6.2
Date: 2023-03-25
Authors@R: c(
           person( "Michail", "Tsagris", role = c("aut", "cre"), email = "mtsagris@uoc.gr" ),
           person( "Muhammad", "Imran", role = "aut", email = "imranshakoor84@yahoo.com" ),
